          "pluginVersion": "4.11.1",
          "queryType": "table",
          "rawSql": "WITH trade_stats AS (\n    SELECT\n      exec_iou_code as token_code,\n      exec_iou_issuer as token_issuer,\n      COUNT(*) as total_trades,\n      COUNT(DISTINCT taker) as unique_takers,\n      SUM(ABS(exec_xrp)) as total_xrp_volume,\n      AVG(exec_price) as avg_price,\n      stddevPop(exec_price) as price_stddev,\n      AVG(ABS(exec_xrp)) as avg_trade_size,\n      stddevPop(ABS(exec_xrp)) as trade_size_stddev,\n      min(time) as first_trade,\n      max(time) as last_trade\n    FROM xrp_watchdog.executed_trades\n    WHERE $__timeFilter(time)\n    GROUP BY token_code, token_issuer\n    HAVING total_trades >= 3\n  )\n  SELECT\n    CASE\n      WHEN length(ts.token_code) = 40 THEN\n        upper(replaceRegexpAll(unhex(ts.token_code), '\\0', ''))\n      ELSE upper(ts.token_code)\n    END as \"Token\",\n    ts.token_issuer as \"Issuer\",\n    ROUND(LEAST(\n      LEAST(50, log10(ts.total_xrp_volume / 1000000 + 1) * 12.5) +\n      CASE\n        WHEN ts.unique_takers <= 2 THEN 30\n        WHEN ts.unique_takers <= 5 THEN 22\n        WHEN ts.unique_takers <= 10 THEN 15\n        WHEN ts.unique_takers <= 20 THEN 8\n        ELSE 3\n      END +\n      CASE\n        WHEN (ts.price_stddev / GREATEST(ts.avg_price, 0.0001)) * 100 < 0.5 THEN 20\n        WHEN (ts.price_stddev / GREATEST(ts.avg_price, 0.0001)) * 100 < 1 THEN 16\n        WHEN (ts.price_stddev / GREATEST(ts.avg_price, 0.0001)) * 100 < 3 THEN 12\n        WHEN (ts.price_stddev / GREATEST(ts.avg_price, 0.0001)) * 100 < 5 THEN 8\n        WHEN (ts.price_stddev / GREATEST(ts.avg_price, 0.0001)) * 100 < 10 THEN 4\n        ELSE 1\n      END +\n      CASE\n        WHEN ts.total_trades / GREATEST((toUnixTimestamp(ts.last_trade) - toUnixTimestamp(ts.first_trade)) / 3600.0, 0.01) >= 100 THEN 15\n        WHEN ts.total_trades / GREATEST((toUnixTimestamp(ts.last_trade) - toUnixTimestamp(ts.first_trade)) / 3600.0, 0.01) >= 50 THEN 12\n        WHEN ts.total_trades / GREATEST((toUnixTimestamp(ts.last_trade) - toUnixTimestamp(ts.first_trade)) / 3600.0, 0.01) >= 20 THEN 8\n        WHEN ts.total_trades / GREATEST((toUnixTimestamp(ts.last_trade) - toUnixTimestamp(ts.first_trade)) / 3600.0, 0.01) >= 10 THEN 5\n        ELSE 2\n      END +\n      CASE\n        WHEN (ts.trade_size_stddev / GREATEST(ts.avg_trade_size, 0.0001)) * 100 < 2 THEN 10\n        WHEN (ts.trade_size_stddev / GREATEST(ts.avg_trade_size, 0.0001)) * 100 < 5 THEN 7\n        WHEN (ts.trade_size_stddev / GREATEST(ts.avg_trade_size, 0.0001)) * 100 < 10 THEN 4\n        ELSE 1\n      END,\n      100\n    ), 1) as \"Risk Score\",\n    ts.total_trades as \"Trades\",\n    ROUND(ts.total_xrp_volume, 0) as \"Volume (XRP)\",\n    ROUND((ts.price_stddev / GREATEST(ts.avg_price, 0.0001)) * 100, 1) as \"Price Var %\",\n    ROUND(ts.total_trades / GREATEST((toUnixTimestamp(ts.last_trade) - toUnixTimestamp(ts.first_trade)) / 3600.0, 0.01), 1) as \"Trades/Hour\",\n    ROUND(CASE\n      WHEN ts.total_trades / GREATEST((toUnixTimestamp(ts.last_trade) - toUnixTimestamp(ts.first_trade)) / 3600.0, 0.01) >= 100 THEN 100\n      WHEN ts.total_trades / GREATEST((toUnixTimestamp(ts.last_trade) - toUnixTimestamp(ts.first_trade)) / 3600.0, 0.01) >= 50 THEN 80\n      WHEN ts.total_trades / GREATEST((toUnixTimestamp(ts.last_trade) - toUnixTimestamp(ts.first_trade)) / 3600.0, 0.01) >= 20 THEN 53\n      WHEN ts.total_trades / GREATEST((toUnixTimestamp(ts.last_trade) - toUnixTimestamp(ts.first_trade)) / 3600.0, 0.01) >= 10 THEN 33\n      ELSE 13\n    END, 0) as \"Burst\",\n    formatDateTime(ts.last_trade, '%Y-%m-%d %H:%M') as \"Updated\"\n  FROM trade_stats ts\n  LEFT JOIN xrp_watchdog.token_stats tst\n    ON ts.token_code = tst.token_code \n    AND ts.token_issuer = tst.token_issuer\n  WHERE ts.token_issuer NOT IN (\n    SELECT token_issuer FROM xrp_watchdog.token_whitelist\n  )\n  AND (\n    tst.classification IS NULL \n    OR tst.classification NOT IN ('bridge', 'legitimate')\n  )\n  AND NOT (\n    upper(CASE WHEN length(ts.token_code) = 40 THEN replaceRegexpAll(unhex(ts.token_code), '\\0', '') ELSE ts.token_code END) LIKE '%.AXL'\n    OR upper(CASE WHEN length(ts.token_code) = 40 THEN replaceRegexpAll(unhex(ts.token_code), '\\0', '') ELSE ts.token_code END) LIKE 'USDC%AXL%'\n    OR upper(CASE WHEN length(ts.token_code) = 40 THEN replaceRegexpAll(unhex(ts.token_code), '\\0', '') ELSE ts.token_code END) LIKE '%BRIDGE%'\n    OR upper(CASE WHEN length(ts.token_code) = 40 THEN replaceRegexpAll(unhex(ts.token_code), '\\0', '') ELSE ts.token_code END) LIKE 'WRAPPED%'\n    OR upper(CASE WHEN length(ts.token_code) = 40 THEN replaceRegexpAll(unhex(ts.token_code), '\\0', '') ELSE ts.token_code END) LIKE '%ALLBRIDGE%'\n  )\n  ORDER BY \"Risk Score\" DESC, ts.total_trades DESC\n  LIMIT 20",
          "refId": "A"
        }
      ],
